{
  "PipelineConfig": {
    //can use relative path for local video, or absolute path(URL) for remote video
    "VideoPath": "match-origin.mp4",
    "SamplingIntervalSeconds": 2,
    // for remote video, higher producer count is better, producer 10 consumer 10 took about 9 minutes processing AUS-ENG.mp4
    // for local video, producer 1 consumer 6 is enough, took approximately 40 seconds processing AUS-ENG.mp4
    "ProducerCount": 1,
    "ConsumerCount": 6
  },
  "OcrConfig": {
    "TessDataPath": "tessdata",
    "RoiConfigPath": "configs/roi_config.json"
  }
}